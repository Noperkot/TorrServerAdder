{
    "version": "1.11.0",
    "name": "TorrServer Adder",
    "short_name": "TSA",
    "description": "__MSG_extension_description__",
    "default_locale": "en",
    "author": "Noperkot",
    "homepage_url": "https://4pda.ru/forum/index.php?showtopic=889960",
    "icons": {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png",
        "16": "icons/icon16.png"
    },
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "128": "icons/icon128.png",
            "48": "icons/icon48.png",
            "16": "icons/icon16.png"
        },
        "default_popup": "options/popup.html"
    },
    "background": {
        "scripts": [
            "common.js",
            "background.js",
            "background_M2.js",
            "torrworker.js",
            "ts_1.1.js",
            "ts_1.2.js"
        ]
    },
    "content_scripts": [
        {
            "all_frames": false,
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_end",
            "css": [
                "notifier.css",
                "wa/fontawesome/css/all_frx.css"
            ],
            "js": [
                "notifier.js",
                "trackers.js",
                "content_scr.js"
            ]
        }
    ],
    "permissions": [
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "downloads",
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking"
    ],
    "web_accessible_resources": [
        "wa/*",
        "wa/fontawesome/webfonts/*"
    ],
    "options_ui": {
        "page": "options/options.html",
        "open_in_tab": true
    },
    "applications": {
        "gecko": {
            "id": "84z74tXJKt8OUrTD@TSA",
            "strict_min_version": "78.0"
        }
    }
}